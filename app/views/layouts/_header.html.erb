  <div class="container">
    <header>
        <div class="row">
            <div class="span3">
                <div id="logo">
                     <%= link_to image_tag("logo.png", alt: full_title(yield(:title)) ), root_path, class: "brand" %>
                </div>
            </div><!--span3-->
            <div class="span9">
              <% if signed_in? %>
                <h1>Signed in</h1>
              <% else %>
                <h1>Not signed in</h1>
              <% end %>
            </div>
        </div><!--row-fluid header-->
    </header><!--header-->
   </div><!--container.header-->